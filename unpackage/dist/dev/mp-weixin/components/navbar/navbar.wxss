.navbar .navbar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  background-color: #f07373;
}
.navbar .navbar-fixed .navbar-content {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 15px;
  height: 45px;
  box-sizing: border-box;
}
.navbar .navbar-fixed .navbar-content .navbar-search {
  display: flex;
  align-items: center;
  padding: 0 10px;
  width: 100%;
  height: 30px;
  border-radius: 30px;
  background-color: #fff;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search-icon {
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search-text {
  width: 100%;
  font-size: 14px;
  color: #999;
}
.navbar .navbar-fixed .navbar-content.search {
  padding-left: 0;
}
.navbar .navbar-fixed .navbar-content.search .navbar-content_search-icons {
  margin-left: 10px;
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content.search .navbar-search {
  border-radius: 5px;
}

